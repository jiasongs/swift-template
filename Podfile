platform :ios, '9.0'

use_frameworks!

target 'SwiftTemplate' do
  
  # Swift
  pod 'Alamofire', '~> 4.9.0' # 网络请求
  pod 'KakaJSON', '~> 1.1.0' # model解析
  pod 'SnapKit', '~> 4.2.0' # 布局
  pod "PromiseKit", "~> 6.10.0" # 处理异步回调
  pod 'RxSwift', '~> 5.0.0' # 响应式编程
  # Objective-C
  pod 'MJRefresh', '~> 3.2.0' # 下拉刷新
  pod 'QMUIKit', '~> 4.0.2' # UI库
  
end
